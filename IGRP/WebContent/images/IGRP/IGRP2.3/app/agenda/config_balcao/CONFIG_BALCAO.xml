<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="../images/IGRP/IGRP2.3/app/agenda/config_balcao/CONFIG_BALCAO.xsl" type="text/xsl"?>
<rows>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>CONFIG_BALCAO</page>
    <app>agenda</app>
    <plsql>
        <action>index</action>
        <package_instance></package_instance>
        <package_db>nosi.webapps.agenda.pages</package_db>
        <package_html>CONFIG_BALCAO</package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>http://localhost:8080/IGRP/webapps?r=igrp/page/preserveUrl&amp;p_id=</preserve_url>
    </plsql>
    <content>
        <title>Configuração de Balcao</title>
        <box_1 type="box" xml-type="form" gen-type="container" gen-group="">
            <fields/>
        </box_1>
        <page_title type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <page_title_text type="text" name="p_page_title_text" persist="true" maxlength="4000">
                    <value>
                        <![CDATA[Configurar Ponto de Atendimento]]>
                    </value>
                </page_title_text>
            </fields>
        </page_title>
        <toolsbar_1 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="gravar">
                <title>Gravar</title>
                <app>agenda</app>
                <page>AddServicos</page>
                <link>index</link>
                <target>submit_form</target>
                <img>success|fa-save</img>
                <parameter>undefinedimages/IGRP/IGRP2.3/app/agenda/addservicos/AddServicos.xsl</parameter>
            </item>
        </toolsbar_1>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <entidade name="p_entidade" type="select" multiple="false" domain="" maxlength="30" required="true" change="false" disabled="false" right="false">
                    <label>Entidade</label>
                    <list>
                        <option/>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </entidade>
                <localizacao name="p_localizacao" type="text" maxlength="300" required="true" change="false" readonly="true" disabled="false" placeholder="" right="false">
                    <label>Localização</label>
                    <value>Ut iste ut amet consectetur</value>
                </localizacao>
                <fuso_horario name="p_fuso_horario" type="text" maxlength="300" required="true" change="false" readonly="true" disabled="false" placeholder="" right="false">
                    <label>Fuso Horário</label>
                    <value>Stract ut ipsum sed rem</value>
                </fuso_horario>
                <nr_servicos name="p_nr_servicos" type="select" multiple="false" domain="" maxlength="2" required="true" change="false" disabled="false" right="false">
                    <label>Número de Serviços</label>
                    <list>
                        <option/>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </nr_servicos>
                <confirmacao name="p_confirmacao" type="select" multiple="false" domain="ALIENACAO_FOGO" maxlength="30" required="true" change="false" disabled="false" right="false">
                    <label>Confirmacao</label>
                    <list>
                        <option/>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </confirmacao>
                <horario_de_atendimento name="p_horario_de_atendimento" type="separator" maxlength="30" placeholder="" right="false">
                    <label>Horário de Atendimento</label>
                </horario_de_atendimento>
                <hr_inicio name="p_hr_inicio" type="select" multiple="false" domain="" maxlength="30" required="true" change="false" disabled="false" right="false">
                    <label>Inicio</label>
                    <list>
                        <option/>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </hr_inicio>
                <hr_fim name="p_hr_fim" type="select" multiple="false" domain="" maxlength="30" required="true" change="false" disabled="false" right="false">
                    <label>Fim</label>
                    <list>
                        <option/>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </hr_fim>
                <hidden name="p_id" type="hidden" maxlength="30" tag="id"/>
                <hidden name="p_estado" type="hidden" maxlength="30" tag="estado"/>
            </fields>
        </form_1>
    </content>
</rows>
