<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="images/IGRP/IGRP2.3/app/agenda/lst_serv/LST_SERV.xsl" type="text/xsl"?>
<rows>
<link_img>/IGRP/images/IGRP/IGRP2.3</link_img>
    <site>
        <welcome_note>Ola</welcome_note>
        <footer_note>2011 - Copyright NOSI</footer_note>
        <user_name>red-igrp</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>RED</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>LST_SERV</page>
    <app>agenda</app>
    <plsql>
        <action>index</action>
        <package_instance></package_instance>
        <package_db>nosi.webapps.agenda.pages</package_db>
        <package_html>LST_SERV</package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>http://localhost:8080/IGRP/webapps?r=igrp/page/preserveUrl&amp;p_id=</preserve_url>
    </plsql>
    <content>
        <title>Lista de Serviços</title>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <entidade name="p_entidade" type="select" multiple="false" domain="" maxlength="30" required="false" change="false" disabled="false" right="false">
                    <label>Entidade</label>
                    <list>
                        <option/>
                        <option>
                            <text>Option 1</text>
                            <value>1</value>
                        </option>
                        <option>
                            <text>Option 2</text>
                            <value>2</value>
                        </option>
                        <option>
                            <text>Option 3</text>
                            <value>3</value>
                        </option>
                        <option>
                            <text>Option 4</text>
                            <value>4</value>
                        </option>
                    </list>
                </entidade>
            </fields>
            <tools-bar>
                <item type="form" code="" class="success" rel="novo">
                    <title>Novo</title>
                    <app>agenda</app>
                    <page>AddServicos</page>
                    <link>index</link>
                    <parameter>undefinedimages/IGRP/IGRP2.3/app/agenda/addservicos/AddServicos.xsl?</parameter>
                    <target>submit</target>
                    <img>success|fa-plus-square</img>
                </item>
                <item type="form" code="" class="default" rel="pesquisar">
                    <title>Pesquisar</title>
                    <app>agenda</app>
                    <page>LST_SERV</page>
                    <link>index</link>
                    <parameter>undefinedimages/IGRP/IGRP2.3/app/agenda/lst_serv/LST_SERV.xsl?</parameter>
                    <target>submit</target>
                    <img>default|fa-search</img>
                </item>
            </tools-bar>
        </form_1>
        <table_1 type="table" xml-type="table" gen-type="container" gen-group="true">
            <fields>
                <servico name="p_servico" type="text" maxlength="30" align="left" lookup_parser="false" iskey="false">
                    <label>Serviço</label>
                </servico>
                <checkbox_1 name="p_checkbox_1" type="checkbox" maxlength="30" align="left" lookup_parser="false" iskey="false" check="true" desc="true">
                    <label>Checkbox</label>
                </checkbox_1>
                <id name="p_id" type="hidden" maxlength="30" tag="id"/>
                <table_1_filter name="p_table_1_filter" type="text" maxlength="100">
                    <value/>
                </table_1_filter>
            </fields>
            <table>
                <value>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <servico name="p_servico">Rem magna mollit anim doloremq</servico>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                        <id name="p_id" tag="id">1</id>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <servico name="p_servico">Laudantium anim unde elit ut</servico>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                        <id name="p_id" tag="id">1</id>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <servico name="p_servico">Consectetur doloremque magna i</servico>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                        <id name="p_id" tag="id">1</id>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <servico name="p_servico">Omnis amet lorem mollit accusa</servico>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                        <id name="p_id" tag="id">1</id>
                    </row>
                    <row>
                        <context-menu>
                            <param>p1=linha1</param>
                            <param>p2=linha1</param>
                        </context-menu>
                        <servico name="p_servico">Adipiscing iste sit sed iste</servico>
                        <checkbox_1 name="p_checkbox_1" check="true" desc="true">1</checkbox_1>
                        <checkbox_1_desc name="p_checkbox_1_desc">1</checkbox_1_desc>
                        <id name="p_id" tag="id">1</id>
                    </row>
                </value>
                <context-menu>
                    <item type="specific" code="" class="success" rel="editar">
                        <title>Editar</title>
                        <app>agenda</app>
                        <page>LST_SERV</page>
                        <link>index</link>
                        <parameter>undefinedimages/IGRP/IGRP2.3/app/agenda/lst_serv/LST_SERV.xsl?</parameter>
                        <target>_blank</target>
                        <img>success|fa-pencil</img>
                    </item>
                    <item type="specific" code="" class="danger" rel="delete">
                        <title>Delete</title>
                        <app>agenda</app>
                        <page>LST_SERV</page>
                        <link>index</link>
                        <parameter>undefinedimages/IGRP/IGRP2.3/app/agenda/lst_serv/LST_SERV.xsl?</parameter>
                        <target>alert_submit</target>
                        <img>danger|fa-remove</img>
                    </item>
                </context-menu>
            </table>
        </table_1>
    </content>
</rows>
