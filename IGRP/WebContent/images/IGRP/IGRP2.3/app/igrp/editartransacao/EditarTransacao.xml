    <page>EditarTransacao</page>
    <app>igrp</app>
    <plsql>
        <action>index</action>
        <package_instance></package_instance>
        <package_db>nosi.webapps.igrp.pages</package_db>
        <package_html>EditarTransacao</package_html>
        <table_name></table_name>
