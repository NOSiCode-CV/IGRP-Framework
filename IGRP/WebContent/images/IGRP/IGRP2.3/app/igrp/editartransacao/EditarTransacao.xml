        <preserve_url>webapps?r=igrp/page/preserveUrl&amp;p_id=21</preserve_url>
    </plsql>
    <content>
        <title>Editar Transacao</title>
        <sectionheader_1 type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <sectionheader_1_text type="text" name="p_sectionheader_1_text" persist="true" maxlength="4000">
                    <value>Editar transação</value>
                </sectionheader_1_text>
            </fields>
        </sectionheader_1>
        <toolsbar_1 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="gravar">
                <title>Gravar</title>
                <app>igrp</app>
                <page>Dominio</page>
                <link>index</link>
                <target>submit</target>
                <img>primary|fa-save</img>
                <preview>/IGRP/images/IGRP/IGRP2.3/app/igrp/dominio/Dominio.xml</preview>
            </item>
        </toolsbar_1>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <descricao name="p_descricao" type="text" maxlength="150" required="true" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Nome</label>
                    <value>Iste perspiciatis unde sed consectetur</value>
                </descricao>
                <status name="p_status" type="checkbox" maxlength="30" required="false" change="false" readonly="false" disabled="false" right="false" check="true">
                    <label>Ativo?</label>
                    <value>1</value>
                </status>
                <hidden name="p_id" type="hidden" maxlength="30" java-type="int" tag="id"/>
            </fields>
        </form_1>
        <view_1 type="view" xml-type="view" title="Detalhes" gen-type="container" gen-group="">
            <fields>
                <aplicacao name="p_aplicacao" type="text" maxlength="30">
                    <label>Aplicação</label>
                    <value>Mollit rem anim sit aliqua</value>
                </aplicacao>
                <codigo name="p_codigo" type="text" maxlength="30">
                    <label>Código</label>
                    <value>Totam laudantium mollit elit s</value>
                </codigo>
            </fields>
        </view_1>
    </content>
