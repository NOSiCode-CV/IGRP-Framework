                <flg_base name="p_flg_base" type="checkbox" maxlength="30" required="false" change="false" readonly="false" disabled="false" right="false" check="true">
                    <label>Menu Base?</label>
                    <value>1</value>
                </flg_base>
                <link name="p_link" type="text" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Link (Procedure)</label>
                    <value>Officia voluptatem sed adipiscing voluptatem</value>
                </link>
                <hidden name="p_area" type="hidden" maxlength="30" tag="area"/>
                <hidden name="p_img_src" type="hidden" maxlength="30" tag="img_src"/>
                <hidden name="p_id" type="hidden" maxlength="30" tag="id"/>
            </fields>
