    <messages>
	    <message type="error">Chave Activaçao nao pode ser nulo. Favor verificar</message>
    </messages>
    <form>
      <label>
        <user>Utilizador</user>
        <password>Password</password>
        <key>Chave Activaçao</key>
        <button>Activar</button>
        <procedure />
        <dml />
      </label>
      <value>
        <user id="p_login" name="p_login" required="true" maxlength="20" />
        <password id="p_password" name="p_password" required="true" maxlength="255" />
        <key id="p_key" name="p_key" required="true" maxlength="9" />
        <button id="p_button" name="p_button" required="false" maxlength="9">Activar</button>
        <procedure id="p_procedure" name="p_procedure" required="false" maxlength="500">db_activation</procedure>
        <dml id="p_dml" name="p_dml" required="false" maxlength="50">AUTH</dml>
      </value>
    </form>
