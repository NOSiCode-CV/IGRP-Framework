<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
<xsl:output doctype-system="about:legacy-compat" encoding="utf-8" omit-xml-declaration="yes" method="html"/><xsl:template match="/"><html><head><xsl:call-template name="IGRP-head"/><link href="{$path}/core/igrp/toolsbar/toolsbar.css?v={$version}" type="text/css" rel="stylesheet"/><link href="{$path}/core/igrp/table/igrp.tables.css?v={$version}" type="text/css" rel="stylesheet"/><link href="{$path}/core/igrp/table/dataTables.bootstrap.css?v={$version}" type="text/css" rel="stylesheet"/><link href="{$path}/core/igrp/table/datatable/dataTables.bootstrap.css?v={$version}" type="text/css" rel="stylesheet"/></head><body class="{$bodyClass} sidebar-off"><xsl:call-template name="IGRP-topmenu"/><form enctype="multipart/form-data" name="formular_default" class="IGRP-form" method="POST"><div class="container-fluid"><div class="row"><xsl:call-template name="IGRP-sidebar"/><div id="igrp-contents" class="col-sm-9 col-md-10 col-md-offset-2 col-sm-offset-3 main"><div class="content"><div class="row row-msg"><div class="gen-column col-md-12"><div class="gen-inner"><xsl:apply-templates select="rows/content/messages" mode="igrp-messages"/></div></div></div><div class="row"><div class="gen-column col-md-12"><div class="gen-inner"><xsl:if test="rows/content/toolsbar_1"><div class="gen-container-item" item-name="toolsbar_1"><div gen-fields=".btns-holder&gt;a.btn" gen-structure="toolsbar" class="toolsbar-holder default"><div role="group" class="btns-holder pull-right"><xsl:apply-templates mode="gen-buttons" select="rows/content/toolsbar_1"><xsl:with-param select="'true'" name="vertical"/><xsl:with-param select="'false'" name="outline"/></xsl:apply-templates></div></div></div></xsl:if><xsl:if test="rows/content/table_1"><div class="gen-container-item" item-name="table_1"><div class="box box-table-contents"><xsl:call-template name="box-header"><xsl:with-param select="rows/content/table_1/@title" name="title"/></xsl:call-template><div class="box-body "><xsl:apply-templates select="rows/content/table_1/fields" mode="form-hidden-fields"/><div class="table-contents-head"><div class="table-contents-inner"/></div><div class="table-box"><div class="table-box-inner"><table class="table table-striped IGRP_contextmenu igrp-data-table" id="table_1"><thead><tr gen-field-template="th" gen-id="drop-zone"><xsl:if test="rows/content/table_1/fields/id"><th item-type="number" item-name="id"><xsl:value-of select="rows/content/table_1/fields/id/label"/></th></xsl:if><xsl:if test="rows/content/table_1/fields/codigo"><th item-type="text" item-name="codigo"><xsl:value-of select="rows/content/table_1/fields/codigo/label"/></th></xsl:if><xsl:if test="rows/content/table_1/fields/nivel"><th item-type="text" item-name="nivel"><xsl:value-of select="rows/content/table_1/fields/nivel/label"/></th></xsl:if><xsl:if test="rows/content/table_1/fields/descricao"><th item-type="text" item-name="descricao"><xsl:value-of select="rows/content/table_1/fields/descricao/label"/></th></xsl:if><xsl:if test="rows/content/table_1/fields/cor"><th item-type="text" item-name="cor"><xsl:value-of select="rows/content/table_1/fields/cor/label"/></th></xsl:if><th class="igrp-table-ctx-th"/></tr></thead><tbody><xsl:for-each select="rows/content/table_1/table/value/row[not(@total='yes')]"><tr gen-id="drop-zone"><xsl:apply-templates select="context-menu" mode="context-param"/><xsl:if test="id"><td data-title="{../../../fields/id/label }" data-row="{position()}" item-type="number" item-name="id"><span><xsl:value-of select="id"/></span></td></xsl:if><xsl:if test="codigo"><td data-title="{../../../fields/codigo/label }" data-row="{position()}" item-type="text" item-name="codigo"><span><xsl:value-of select="codigo"/></span></td></xsl:if><xsl:if test="nivel"><td data-title="{../../../fields/nivel/label }" data-row="{position()}" item-type="text" item-name="nivel"><span><xsl:value-of select="nivel"/></span></td></xsl:if><xsl:if test="descricao"><td data-title="{../../../fields/descricao/label }" data-row="{positi