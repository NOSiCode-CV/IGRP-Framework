<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="/IGRP/images/IGRP/IGRP2.3/app/igrp/novodominio/NovoDominio.xsl" type="text/xsl"?>
<rows>
    <link_img>/IGRP/images/IGRP/IGRP2.3</link_img>
    <site>
        <welcome_note>Oba</welcome_note>
        <footer_note>2017 - Copyright NOSI</footer_note>
        <user_name>Hexagon</user_name>
        <button type="specific">
            <title>Sair</title>
            <app>DUA</app>
            <page />
            <link>#</link>
            <target>_self</target>
            <img>exit.png</img>
        </button>
    </site>
    <page>NovoDominio</page>
    <app>igrp</app>
    <plsql>
        <action>index</action>
        <package_instance></package_instance>
        <package_db>nosi.webapps.igrp.pages</package_db>
        <package_html>NovoDominio</package_html>
        <table_name></table_name>
        <with_replace>false</with_replace>
        <with_label>false</with_label>
        <with_biztalk>false</with_biztalk>
        <dynamic_menu>false</dynamic_menu>
        <copy_menu>false</copy_menu>
        <package_copy_db />
        <package_copy_html />
        <preserve_url>webapps?r=igrp/page/preserveUrl&amp;p_id=5</preserve_url>
    </plsql>
    <content>
        <title>Registar Dominio</title>
        <sectionheader_1 type="sectionheader" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <sectionheader_1_text type="text" name="p_sectionheader_1_text" persist="true" maxlength="4000">
                    <value>Gestão de Dominio - Novo</value>
                </sectionheader_1_text>
            </fields>
        </sectionheader_1>
        <toolsbar_1 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="gravar">
                <title>Gravar</title>
                <app>igrp</app>
                <page>Dominio</page>
                <link>index</link>
                <target>submit_form</target>
                <img>primary|fa-save</img>
                <preview>/IGRP/images/IGRP/IGRP2.3/app/igrp/dominio/Dominio.xml</preview>
            </item>
            <item type="specific" code="" rel="lista_dominios">
                <title>Lista Domínios</title>
                <app>igrp</app>
                <page>Dominio</page>
                <link>index</link>
                <target>_self</target>
                <img>default|fa-list</img>
                <preview>/IGRP/images/IGRP/IGRP2.3/app/igrp/dominio/Dominio.xml</preview>
            </item>
        </toolsbar_1>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
            <fields>
                <dominio name="p_dominio" type="text" maxlength="30" required="true" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Domínio</label>
                    <value>Elit lorem totam unde sed</value>
                </dominio>
                <valor name="p_valor" type="text" maxlength="30" required="true" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Valor</label>
                    <value>Magna officia magna sit anim</value>
                </valor>
                <description name="p_description" type="text" maxlength="30" required="true" change="false" readonly="false" disabled="false" placeholder="" right="false">
                    <label>Significado</label>
                    <value>Accusantium magna ipsum lorem</value>
                </description>
                <estado name="p_estado" type="select" multiple="false" maxlength="30" required="false" change="false" disabled="false" right="false" domain="SIN_NAO" java-type="">
                    <label>Estado</label>
                    <list>
                        <option/>
                        <option>
                            <text>Não</text>
                            <value>Nao</value>
                        </option>
                        <option>
                            <text>Sim</text>
                            <value>Sim</value>
                        </option>
                    </list>
                </estado>
                <ordem name="p_ordem" type="number" min="0" max="20" maxlength="30" required="false" change="false" readonly="false" disabled="false" placeholder="" right="false" java-type="int">
                    <label>Ordem</label>
                    <value>3</value>
                </ordem>
                <hidden name="p_id" type="hidden" maxlength="30" java-type="int" tag="id"/>
            </fields>
        </form_1>
    </content>
</rows>
