                    <row>
                        <transacao name="p_transacao" check="true" desc="true">1</transacao>
                        <transacao_desc name="p_transacao_desc">1</transacao_desc>
                        <nome name="p_nome">Lorem unde anim aliqua dolor</nome>
                    </row>
                </value>
            </table>
        </table_1>
        <toolsbar_3 type="toolsbar" xml-type="toolsbar" gen-type="menu" gen-group="">
            <item type="specific" code="" rel="gestao_de_transacoes">
                <title>Gestão de transações</title>
                <app>igrp</app>
                <page>Transaccao</page>
                <link>index</link>
                <target>_blank</target>
                <img>black|fa-exchange</img>
                <preview>/IGRP/images/IGRP/IGRP2.3/app/igrp/transaccao/Transaccao.xml</preview>
            </item>
        </toolsbar_3>
        <form_1 type="form" xml-type="form" gen-type="container" gen-group="">
