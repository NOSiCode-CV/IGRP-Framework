                </transacao>
                <nome name="p_nome" type="text" maxlength="30" align="left" lookup_parser="false" iskey="false">
                    <label>Nome (code)</label>
                </nome>
            </fields>
            <table>
                <value>
                    <row>
                        <transacao name="p_transacao" check="true" desc="true">1</transacao>
                        <transacao_desc name="p_transacao_desc">1</transacao_desc>
                        <nome name="p_nome">Natus lorem voluptatem labore</nome>
                    </row>
                    <row>
                        <transacao name="p_transacao" check="true" desc="true">1</transacao>
                        <transacao_desc name="p_transacao_desc">1</transacao_desc>
